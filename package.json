{
  "name": "gim",
  "type": "module",
  "private": true,
  "module": "index.ts",
  "scripts": {
    "lint": "eslint",
    "dev": "bun --hot app/index.ts",
    "build": "bun build app/index.ts --target bun --outdir dist",
    "start": "bun run build && bun dist/index.js",
    "compile": "bun build app/index.ts --compile --outfile dist/${npm_package_name}",
    "lint:fix": "eslint --fix",
    "s3": "bun run tools/s3-upload.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@hono/swagger-ui": "^0.5.1",
    "hono": "^4.7.10",
    "unstorage": "^1.16.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@types/bun": "latest",
    "eslint": "^9.27.0",
    "wrangler": "^4.16.1"
  }
}
